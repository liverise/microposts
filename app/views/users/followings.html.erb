<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>

<div class="row">
  <aside class="col-md-4">  
    <ul>
      <% @followings.each do |u| %>
      <%= gravatar_for @user %>
      <%= u.name %>
      <%= f.submit "Unfollow", class: "btn" %>
      <% end %>
    </ul>
  </aside>
</div>